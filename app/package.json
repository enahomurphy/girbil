    "framework7-icons": "^3.0.0",
    "framework7-react": "^5.4.5",
    "fs": "^0.0.1-security",
    "graphql": "^14.6.0",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hot-loader": "^4.12.19",
    "react-player": "^1.15.2",
    "react-use": "^13.26.1",
    "recordrtc": "^5.5.9",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.0.1",
    "template7": "^1.4.2",
    "timeago.js": "^4.0.2",
    "xstate": "^4.7.8"
  },
